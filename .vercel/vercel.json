{
  "buildCommand": "npm run build",
  "outputDirectory": "dist",
  "installCommand": "npm install",
  "framework": null,
  "routes": [
    { "handle": "filesystem" },
    { "src": "/api/(.*)", "dest": "https://onnoto-backend-production.up.railway.app/api/$1" },
    { "src": "/(.*)", "dest": "/index.html" }
  ]
}